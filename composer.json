{
    "name": "sermajid/laravel-whmcs",
    "description": "laravel api whmcs",
    "type": "library",
    "authors": [{
        "name": "Majid Feizi",
        "email": "sermajid3@gmail.com"
    }],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "homepage": "https://github.com/sermajid/laravel-whmcs",
    "autoload": {
        "psr-4": {
            "sermajid\\LaravelWhmcs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "sermajid\\LaravelWhmcs\\WhmcsServiceProvider"
            ],
            "aliases": {
                "Whmcs": "sermajid\\LaravelWhmcs\\Facades\\Whmcs"
            }
        }
    },
    "require": {
        "guzzlehttp/guzzle": "*",
        "ext-json": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://github.com/sermajid/laravel-whmcs.git"
        }
        ]
}
